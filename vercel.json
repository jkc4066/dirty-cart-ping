{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "u/index.html", "use": "@vercel/static" },
    { "src": "u.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/u/?$", "dest": "/u/index.html" },
    { "src": "^/u/index.html$", "dest": "/u/index.html" },
    { "src": "^/u.html$", "dest": "/u.html" }
  ]
}
